{
    "name": "AutoPlay YouTube Shorts",
    "description": "Enable auto-play for youtube shorts on desktop version.",
    "version": "1.0",
    "manifest_version": 3,
    "content_scripts": [
        {
          "matches": ["https://www.youtube.com/*"],
          "js": ["index.js"]
        }
      ],
      "icons": {
        "16": "/icons/icon-16.png",
        "32": "/icons/icon-32.png",
        "48": "/icons/icon-48.png",
        "128": "/icons/icon-128.png"
    }

  }